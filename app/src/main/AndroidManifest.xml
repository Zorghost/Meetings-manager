<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.htw_berlin.meetingmanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/Meeting_Manager"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".meeting.meetingDetail"
            android:exported="false" />
        <activity
            android:name=".meeting.MeetingDetailActivity"
            android:exported="false" />
        <activity
            android:name=".addMeeting.Person.Profile"
            android:exported="false" />
        <activity android:name=".meeting.MeetingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".addMeeting.Person.PersonActivity"
            android:exported="false" />
        <activity
            android:name=".Login.LoginActivity"
            android:exported="false" />
        <activity android:name=".Register.RegisterActivity" />
        <activity android:name=".addMeeting.AddMeetingActivity" />
    </application>

</manifest>